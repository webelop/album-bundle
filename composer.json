{
    "name": "webelop/album-bundle",
    "description": "Webelop Album is a photo management and publishing Symfony bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Charles Callu",
            "email": "jcc@webelop.net"
        }
    ],
    "bin": ["bin/photosync"],
    "require": {
        "php": "^7.1.3",
        "doctrine/common": "^2.8",
        "doctrine/doctrine-bundle": "^1.8|^2.0",
        "doctrine/orm": "^2.6.3",
        "symfony/routing": "^4.2|^5.0",
        "symfony/twig-bundle": "^4.2|^5.0",
        "symfony/asset": "^4.2|^5.0",
        "symfony/security-bundle": "^4.2|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Webelop\\AlbumBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webelop\\AlbumBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/yaml": "^5.0",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/validator": "^5.0"
    }
}
